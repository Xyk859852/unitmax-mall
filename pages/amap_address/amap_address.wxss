/* pages/amap_address/amap_address.wxss */
.section{
  height: 30px;
  width: 92%;
  background-color: #ffffff;
  padding:10px 4%;
}
.section input{
  width:92%;
  height:20px;
  line-height: 20px;
  font-size: 14px;
  border-radius: 3px;
  padding: 5px 4%;
  background-color: #f5f5f5;
}
.title{
  padding: 10px 4%;
  font-size: 14px;
}
.address{
  background-color: #ffffff;
  padding: 10px 4%;
  font-size: 14px;
}
.text_box{
  background-color: #ffffff;
  padding: 10px 4%;
  font-size: 16px;
  border-bottom: #f5f5f5 1px solid;
}
.text_box view{
  font-size: 14px;
}