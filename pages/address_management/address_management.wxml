<!--pages/address_management/address_management.wxml-->
<view class='address_border' style='background-image: url(../../images/address_border.png);'></view>
<view class='body'>
  <view wx:for="{{list}}" class='address_box'>
    <view class='address'>
      <view>收货人：{{item.CONSIGNEE}}<label>{{item.TAKEPHONE}}</label></view>
      <view class='address_detail'>收件地址：{{item.MOBLE}}{{item.ADDRESS_DTEAIL}}</view>
    </view>
    <view class='btn_box'>
      <view class='small_btn'>编辑</view>
      <view class='small_btn'>删除</view>
    </view>
  </view>
</view>
<view class='footer'>
 <navigator url="../add_address/add_address">
<button>新增地址</button>
</navigator>
</view>